import java.util.Scanner;

public class SumOfTwoNumbers {
	
	public static void main (String [] arg) {
		
		int num1, num2, sum;
		
		System.out.println(" enter two numbers");
		
		Scanner in = new Scanner(System.in);
		
		num1 = in.nextInt();
		num2 = in.nextInt();
		sum = num1 + num2;
		System.out.println(" sum of numbers is " + sum);
	}

}
