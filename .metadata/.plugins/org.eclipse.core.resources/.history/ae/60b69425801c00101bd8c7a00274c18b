
public class FibbonacciSeries {
	
	public static void main(String [] arguments) {
		
		int n=10,  firstNum=0, secNum=1;
		int nextNum=0;
		 for(int i=1; i<=n; i++ ) {
			 
			 System.out.print(firstNum + ",");
			 nextNum=firstNum + secNum ;
			 
			 firstNum = secNum;
			 secNum = nextNum;
		 }
	}
	
}
